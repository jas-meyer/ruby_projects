<h1> Editing <%=@dojo.branch%> </h1>

<form action = "/dojos/<%=@dojo.id%>" method = "post">
	<input type="hidden" name ="authenticity_token" value = "<%= form_authenticity_token %>">
	<input type ="hidden" name="_method" value="patch">
<h3> Branch: <input type ="text" name ="dojo[branch]" value = "<%=@dojo.branch%>"></h3>
<h3> Street: <input type ="text" name = "dojo[street]" value = "<%=@dojo.street%>"></h3>
<h3> City: <input type ="text" name = "dojo[city]" value = "<%=@dojo.city%>"></h3>
<h3> State: <input type="text" name = "dojo[state]" value = "<%=@dojo.state%>"></h3>
<button type="submit">Update</button>
</form>

<a href ='/dojos'>Home</a> | <a href ='/dojos/<%=@dojo.id%>'>Show</a>