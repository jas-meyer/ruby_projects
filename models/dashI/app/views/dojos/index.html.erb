
<head>
<title>Dashboard</title>
 <%= stylesheet_link_tag "style" %>
</head>
<h1> Listing <%=@count%> Dojos<h1>
<% flash.each do |value| %>
	<%=value%>
<% end %> 
<table>
	<tr>
		<th> Coding Dojo Branch </th>
		<th> Street </th>
		<th> City </th>
		<th> State </th>
		<th> Actions </th>
	</tr>

<% @dojos.each do |dojo| %>
	<tr>
  		<td>CD <%= dojo.branch %></td>
  		<td><%= dojo.street %></td>
  		<td><%= dojo.city %></td>
  		<td><%= dojo.state %></td>
  		<td><a href ='/dojos/<%=dojo.id%>'>Show</a>
  			<a href ='/dojos/<%=dojo.id%>/edit'>Edit</a>
  			<a href ="/dojos/<%=dojo.id%>" data-method="delete">Destroy</a>
  	</tr>
<% end %>
</table>

<a href='dojos/new'>New Dojo</a>